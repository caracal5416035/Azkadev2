<section id="contact" tabindex="-1" aria-label="Contact me">
  <h2>Contact Me</h2>
  <form id="contact-form" aria-label="Contact form">
    <label for="name">Nama</label>
    <input type="text" id="name" name="name" required aria-required="true" aria-describedby="name-error" />
    <span id="name-error" style="color:#d32f2f; font-size:0.9rem; display:none;">Please enter your name.</span>

    <label for="email">Email</label>
    <input type="email" id="email" name="email" required aria-required="true" aria-describedby="email-error" />
    <span id="email-error" style="color:#d32f2f; font-size:0.9rem; display:none;">Please enter a valid email address.</span>

    <label for="message">Pesan</label>
    <textarea id="message" name="message" rows="4" required aria-required="true" aria-describedby="message-error"></textarea>
    <span id="message-error" style="color:#d32f2f; font-size:0.9rem; display:none;">Please enter a message.</span>

    <button type="submit" aria-label="Send message">Kirim</button>
    <p id="form-status" role="alert" style="margin-top:1rem; font-weight:600; color:#1976d2; display:none;"></p>
  </form>
