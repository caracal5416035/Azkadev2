document.addEventListener("keydown",function(e){if(e.key==="F12"){e.preventDefault();}if(e.ctrlKey&&e.shiftKey&&e.keyCode===73){e.preventDefault();}if(e.ctrlKey&&e.shiftKey&&e.keyCode===74){e.preventDefault();}if(e.ctrlKey&&e.keyCode===85){e.preventDefault();}if(e.ctrlKey&&e.shiftKey&&e.keyCode===85){e.preventDefault();}if(e.key==="F11"){e.preventDefault();}if(e.ctrlKey&&e.shiftKey&&e.keyCode===67){e.preventDefault();}if(e.ctrlKey&&e.keyCode===80){e.preventDefault();}});
